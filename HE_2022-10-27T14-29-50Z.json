			"enable": false,
			"name": "常用网站 http >> https",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^http://(.*)?(youtube|google|wordpress|github|twitter|deviantart|facebook|pinterest|vimeo|feedly|flickr|quora|instagram|duckduckgo|telegram|archive|wikipedia)(\\.org|\\.com)",
